FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/coolant";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 340;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 313.6;
    }
    outlet
    {
        type            zeroGradient;
    }
    coolant_to_clad
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     fluidThermo;
    }
    coolant_outer
    {
        type            zeroGradient;
    }
}

