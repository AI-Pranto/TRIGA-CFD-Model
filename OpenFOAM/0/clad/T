FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/clad";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 340;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
        value           $internalField;
    }
    clad_to_coolant
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
    clad_to_fuel
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
}


